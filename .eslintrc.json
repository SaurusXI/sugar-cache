{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "indent": ["error", 4],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "target": "es6",
    "declaration": true,
    "noImplicitAny": false,
    "removeComments": true,
    "esModuleInterop": true,
    "noLib": false,
    "preserveConstEnums": true,
    "rules": {
    },
    "include": ["lib/**/*.ts"]
}
